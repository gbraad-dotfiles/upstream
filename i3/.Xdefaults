*.foreground:   #000000
*.background:   #ffffff
*.cursorColor:  #aeafad

! Black / Grey
*.color0:       #ffffff
*.color8:       #666666

! Red / Bright Red
*.color1:       #d54e53
*.color9:       #FF3334

! Green + Bright Green
*.color2:       #b9ca4a
*.color10:      #9ec400

! Yellow (Orange) + Bright Yellow (Yellow)
*.color3:       #e78c45
*.color11:      #e7c547

! Blue + Bright Blue
*.color4:       #7aa6da
*.color12:      #7aa6da

! Magenta (Purple) + Bright Magenta
*.color5:       #c397d8
*.color13:      #b777e0

! Cyan (Aqua) + Bright Cyan
*.color6:       #70c0b1
*.color14:      #54ced6

! Light Grey (Selection) + White (Current Line)
*.color7:       #424242
*.color15:      #2a2a2a

URxvt*background: #ffffff
URxvt*foreground: #000000
URxvt*font: xft:Source\ Code\ Pro:pixelsize=13:antialias=true:hinting=true,xft:Source\ Code\ Pro\ Medium:pixelsize=13:antialias=true:hinting=true
URxvt*scrollBar: false

xterm*faceName: Source Code Pro
